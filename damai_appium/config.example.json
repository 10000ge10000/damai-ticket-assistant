{
  // damai_appium 配置示例（带注释）
  // 使用方式：复制此文件为同目录下的 `config.jsonc`（覆盖或本地修改），切勿将含真实个人信息的文件提交到远程仓库。

  // Appium server 地址（通常为本机或远程 Appium Server），格式 host:port
  "server_url": "127.0.0.1:4723",

  // 搜索关键字：演出名称或歌手名，用于在大麦页面上搜索目标演出
  "keyword": "演出/歌手/关键字",

  // 观演人列表：填写购票时需要提交的观演人姓名（可为空列表用于测试）
  "users": [
    "观演人姓名（示例）"
  ],

  // 城市（字符串，可空）
  "city": "北京",

  // 演出日期（可选，格式示例："10.18"），当只有一个城市时可能不重要
  "date": "",

  // 价格（用于显示或参考，实际选择通常使用 price_index）
  "price": "",

  // 价格索引（从0开始，按从低到高排序），用于在页面上选择对应票价
  "price_index": 0,

  // 是否自动提交订单：在测试或评估时请设为 false，生产环境谨慎设为 true
  "if_commit_order": false
}